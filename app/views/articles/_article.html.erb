<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= article.body %>
  </p>

  <p>
    <strong>Sentiment:</strong>
    <%= article.sentiment %>
  </p>

  <p>
    <strong>Rssfeed:</strong>
    <%= article.rss_feed.title %>
  </p>

  <p>
    <strong>Group:</strong>
    <%= article.group_id %>
  </p>

</div>
